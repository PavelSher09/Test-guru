<tr>
  <td><%= link_to test.title, admin_test_path(test) %></td>
  <td><%= test.category.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to 'Show', admin_test_path(test) %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
  <td>
    <%= link_to 'Delete',
                  admin_test_path(test), method: :delete,
                  data: {confirm: "Delete test #{test.title}?"} %>
  </td>
  
</tr>
